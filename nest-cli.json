{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/control-panel/src",
  "root": "apps/control-panel",
  "projects": {
    "boat": {
      "type": "library",
      "root": "libs/boat",
      "entryFile": "index",
      "sourceRoot": "libs/boat/src",
      "compilerOptions": {
        "tsConfigPath": "libs/boat/tsconfig.lib.json"
      }
    },
    "control-panel": {
      "type": "application",
      "root": "apps/control-panel",
      "entryFile": "main",
      "sourceRoot": "apps/control-panel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/control-panel/tsconfig.app.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "auth-apis": {
      "type": "application",
      "root": "apps/auth-apis",
      "entryFile": "main",
      "sourceRoot": "apps/auth-apis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-apis/tsconfig.app.json"
      }
    }
  },

  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/tsconfig.app.json"
  }
}